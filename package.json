{
  "name": "react-minimal-dropdown",
  "version": "1.3.2",
  "description": "Simple, minimal and unobtrusive dropdown for React",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --watch --open --open-page=./example",
    "build:example": "cross-env webpack",
    "build": "cross-env NODE_ENV=production webpack -p --env=production",
    "precommit": "lint-staged",
    "postpublish": "git push --tags && git push"
  },
  "keywords": [
    "react",
    "dropdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/manuelbieh/react-minimal-dropdown.git"
  },
  "files": [
    "dist/index.js",
    "dist/css/index.css",
    "src/*",
    ".babelrc",
    ".eslintrc.json",
    "LICENSE",
    "README.md"
  ],
  "lint-staged": {
    "src/*.{js,css}": [
      "prettier --write"
    ],
    "src/*.js": [
      "eslint"
    ]
  },
  "author": "Manuel Bieh <info@manuelbieh.de> (https://github.com/manuelbieh)",
  "license": "MIT",
  "devDependencies": {
    "@manuel-bieh/layout": "^1.0.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "classnames": "^2.2.5",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-wiremore": "1.5.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-flowtype": "2.47.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.8.2",
    "eslint-plugin-security": "1.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "flow-bin": "^0.73.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^8.1.0",
    "normalize-css": "^2.3.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "style-loader": "^0.21.0",
    "uppercamelcase": "^1.1.0",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.4"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  }
}
