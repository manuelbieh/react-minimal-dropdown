{
  "name": "react-minimal-dropdown",
  "version": "1.3.1",
  "description": "Simple, minimal and unobtrusive dropdown for React",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --watch --open --open-page=./example",
    "build:example": "cross-env webpack",
    "build": "cross-env NODE_ENV=production webpack -p --env=production",
    "precommit": "lint-staged",
    "prepare": "npm run build",
    "postpublish": "git push --tags && git push"
  },
  "keywords": [
    "react",
    "dropdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/manuelbieh/react-minimal-dropdown.git"
  },
  "files": [
    "dist/index.js",
    "dist/css/index.css",
    "src/*",
    ".babelrc",
    ".eslintrc.json",
    "LICENSE",
    "README.md"
  ],
  "lint-staged": {
    "src/*.{js,css}": [
      "prettier --write"
    ],
    "src/*.js": [
      "eslint"
    ]
  },
  "author": "Manuel Bieh <info@manuelbieh.de> (https://github.com/manuelbieh)",
  "license": "MIT",
  "devDependencies": {
    "@manuel-bieh/layout": "^1.0.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "classnames": "^2.2.5",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-wiremore": "^1.3.1",
    "eslint-plugin-babel": "^4.1.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "flow-bin": "^0.43.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "normalize-css": "^2.3.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "style-loader": "^0.16.1",
    "uppercamelcase": "^1.1.0",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1"
  }
}
