{
  "name": "react-minimal-dropdown",
  "version": "1.3.2",
  "description": "Simple, minimal and unobtrusive dropdown for React",
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --watch --open --open-page=./example",
    "build:example": "cross-env webpack",
    "build": "cross-env NODE_ENV=production webpack -p --env=production",
    "precommit": "lint-staged",
    "postpublish": "git push --tags && git push"
  },
  "keywords": [
    "react",
    "dropdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/manuelbieh/react-minimal-dropdown.git"
  },
  "files": [
    "dist/index.js",
    "dist/css/index.css",
    "src/*",
    ".babelrc",
    ".eslintrc.json",
    "LICENSE",
    "README.md"
  ],
  "lint-staged": {
    "src/*.{js,css}": [
      "prettier --write"
    ],
    "src/*.js": [
      "eslint"
    ]
  },
  "author": "Manuel Bieh <info@manuelbieh.de> (https://github.com/manuelbieh)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@manuel-bieh/layout": "^1.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "classnames": "^2.2.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-wiremore": "^2.2.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^0.0.0",
    "eslint-plugin-security": "1.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "flow-bin": "^0.88.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "normalize-css": "^2.3.1",
    "prettier": "^1.15.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "style-loader": "^0.23.1",
    "uppercamelcase": "^3.0.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  }
}
